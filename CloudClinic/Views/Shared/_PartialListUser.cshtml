@model IEnumerable<CloudClinic.Models.UserViewModel>

@{ var msg = new CloudClinic.Models.ViewModel.MessageBoxViewModel();
 msg.TitleMessage = ViewBag.TitleMessage;
 msg.MessageBody = ViewBag.MessageBody;
 msg.AlertClass = ViewBag.AlertClass; }

@Html.Partial("_MessageBox", msg)

<table class="table">
    <tr>
        <th>
            User ID
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            
        </tr>
    }
</table>

