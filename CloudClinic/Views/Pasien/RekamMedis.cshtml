@model List<CloudClinic.Models.DataModel.Diagnosis>


<table class="table">
    <tr>
        <th>
            User Name
        </th>
        <th>
            Tgl. Datang
        </th>
        <th>
            Anamnesa
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pasien.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TglDatang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amnanesa)
            </td>
        </tr>
    }

</table>
